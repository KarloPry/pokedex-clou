generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id        Int    @id @default(autoincrement())
  name      String
  URL       String
  type      String
  Total     Int
  HP        Int
  Attack    Int
  Defense   Int
  Sp_Atk    Int
  Sp_Def    Int
  Speed     Int
  MainColor String
}
